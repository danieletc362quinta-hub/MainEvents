{
  "name": "mainevents",
  "version": "1.0.0",
  "description": "Plataforma de eventos con Node.js, Express y React",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:debug": "nodemon --inspect src/menu.js",
    "dev:watch": "nodemon --watch src --ext js,json src/menu.js",
    "build": "npm run clean && npm run lint && npm run test",
    "clean": "rm -rf dist build coverage",
    "lint": "eslint src/ --ext .js --fix",
    "lint:check": "eslint src/ --ext .js",
    "format": "prettier --write \"src/**/*.{js,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,json}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "test:performance": "jest --testPathPattern=performance",
    "test:security": "jest --testPathPattern=security",
    "test:ci": "npm run lint:check && npm run test:coverage",
    "test:all": "node scripts/run-tests.js",
    "db:seed": "node scripts/seed.js",
    "db:migrate": "node scripts/migrate.js",
    "db:backup": "node scripts/backup.js",
    "db:restore": "node scripts/restore.js",
    "optimize": "npm run lint && npm run format && npm run test",
    "analyze": "npm run analyze:bundles && npm run analyze:deps",
    "analyze:bundles": "npx webpack-bundle-analyzer dist/stats.json",
    "analyze:deps": "npx depcheck",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "security:check": "npm audit --audit-level=moderate",
    "performance:test": "artillery run tests/performance/load-test.yml",
    "performance:monitor": "node scripts/monitor.js",
    "health:check": "curl -f http://localhost:4000/health || exit 1",
    "docker:build": "docker build -t mainevents .",
    "docker:run": "docker run -p 4000:4000 mainevents",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "deploy:staging": "npm run build && npm run test:ci && echo 'Deploy to staging'",
    "deploy:production": "npm run build && npm run test:ci && npm run security:check && echo 'Deploy to production'",
    "monitor:start": "node scripts/monitor.js",
    "logs:tail": "tail -f logs/app.log",
    "logs:clear": "rm -rf logs/*.log",
    "cache:clear": "node scripts/clear-cache.js",
    "backup:create": "node scripts/backup.js",
    "backup:list": "ls -la backups/",
    "maintenance:start": "echo 'maintenance' > maintenance.flag",
    "maintenance:stop": "rm -f maintenance.flag",
    "docs:generate": "jsdoc -c jsdoc.json",
    "docs:serve": "http-server docs/ -p 8080",
    "benchmark": "node scripts/benchmark.js",
    "profile": "node --prof src/menu.js",
    "profile:analyze": "node --prof-process isolate-*.log > profile.txt",
    "check:errors": "node scripts/check-errors.js",
    "check:all": "npm run check:errors && npm run lint:check && npm run test"
  },
  "keywords": [
    "events",
    "nodejs",
    "express",
    "mongodb",
    "react",
    "typescript",
    "api",
    "rest",
    "jwt",
    "authentication",
    "payments",
    "mercadopago"
  ],
  "author": "Tu Nombre",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsonwebtoken": "^9.0.2",
    "mercadopago": "^2.0.7",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.17.2",
    "qrcode": "^1.5.4",
    "react-i18next": "^16.2.3",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^2.0.16",
    "artillery": "^2.0.0-27",
    "babel-jest": "^30.1.2",
    "depcheck": "^1.4.7",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tu-usuario/mainevents.git"
  },
  "bugs": {
    "url": "https://github.com/tu-usuario/mainevents/issues"
  },
  "homepage": "https://github.com/tu-usuario/mainevents#readme",
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "env": {
      "node": true,
      "es2022": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error",
      "prefer-const": "error"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  }
}
