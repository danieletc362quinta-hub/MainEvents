# Configuración de la API
REACT_APP_API_URL=https://api.main-events.com/v1

# Configuración de autenticación
REACT_APP_JWT_SECRET=your_production_jwt_secret
REACT_APP_JWT_EXPIRES_IN=7d

# Configuración de Mercado Pago (producción)
REACT_APP_MERCADOPAGO_PUBLIC_KEY=your_production_public_key

# Configuración de PayPal (producción)
REACT_APP_PAYPAL_CLIENT_ID=your_production_client_id

# Configuración de Google Analytics
REACT_APP_GA_TRACKING_ID=your_ga_tracking_id

# Configuración de Sentry (monitoreo de errores)
REACT_APP_SENTRY_DSN=your_sentry_dsn

# Configuración de Firebase (opcional)
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain
REACT_APP_FIREBASE_PROJECT_ID=your_firebase_project_id

# Configuración de la aplicación
NODE_ENV=production
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false

# Configuración de caché (opcional)
REACT_APP_CACHE_VERSION=1
REACT_APP_CACHE_NAME=main-events-cache
